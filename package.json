{
  "name": "eip721-subgraph",
  "version": "0.1.0",
  "scripts": {
    "test-contracts": "rocketh launch",
    "deploy-test-contracts": "rocketh launch -n http://localhost:8545 -q test_deployments.json",
    "create-local-graph": "graph create wighawag/eip721-subgraph --node http://127.0.0.1:8020",
    "codegen-graph": "graph codegen",
    "build-graph": "graph build",
    "deploy-graph": "dotenv -- cross-var graph deploy --access-token $THEGRAPH_TOKEN wighawag/eip721-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-local-graph": "graph deploy wighawag/eip721-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.17.1",
    "@graphprotocol/graph-ts": "^0.17.0",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "ethers": "^5.0.0-beta.159",
    "ganache-core": "^2.8.0",
    "rocketh": "^0.4.1",
    "solc": "^0.5.12"
  },
  "dependencies": {}
}
